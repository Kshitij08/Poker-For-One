{
  "buildCommand": "cd 'main game' && npm run build",
  "outputDirectory": "main game/dist",
  "installCommand": "cd 'main game' && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/favicon.ico",
      "destination": "/favicon.svg"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
